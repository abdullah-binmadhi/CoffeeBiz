{
  "name": "coffeebiz-analytics",
  "version": "1.0.0",
  "description": "Business Intelligence Dashboard for Coffee Shops",
  "homepage": "https://abdullah-binmadhi.github.io/CoffeeBiz",
  "private": true,
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "chart.js": "^4.4.0",
    "react-chartjs-2": "^5.2.0",
    "date-fns": "^2.30.0",
    "papaparse": "^5.4.1",
    "@types/papaparse": "^5.3.14",
    "pg": "^8.11.3",
    "@types/pg": "^8.10.7",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "db:setup": "ts-node database/setup.ts setup",
    "db:reset": "ts-node database/setup.ts reset",
    "db:migrate": "ts-node database/setup.ts migrate",
    "server": "nodemon --exec ts-node server/index.ts",
    "dev": "concurrently \"npm run server\" \"npm start\"",
    "test:api": "vitest run --config vitest.config.ts",
    "test:api:watch": "vitest --config vitest.config.ts"
  },
  "devDependencies": {
    "gh-pages": "^6.0.0",
    "tailwindcss": "^3.3.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "@types/node": "^20.8.0",
    "ts-node": "^10.9.1",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.16",
    "vitest": "^1.0.4",
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}