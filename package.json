{
  "name": "coffeebiz-analytics",
  "version": "1.0.0",
  "description": "Business Intelligence Dashboard for Coffee Shops",
  "private": true,
  "dependencies": {
    "@types/bull": "^3.15.9",
    "@types/ioredis": "^4.28.10",
    "@types/papaparse": "^5.3.14",
    "@types/pg": "^8.10.7",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "bull": "^4.16.5",
    "chart.js": "^4.4.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.7.0",
    "node-fetch": "^3.3.2",
    "papaparse": "^5.5.3",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "redis": "^5.8.2",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "db:setup": "ts-node database/setup.ts setup",
    "db:reset": "ts-node database/setup.ts reset",
    "db:migrate": "ts-node database/setup.ts migrate",
    "db:optimize": "ts-node database/setup.ts optimize",
    "server": "nodemon --exec ts-node server/index.ts",
    "dev": "concurrently \"npm run server\" \"npm start\"",
    "test:api": "vitest run --config vitest.config.ts",
    "test:api:watch": "vitest --config vitest.config.ts",
    "test:frontend": "react-scripts test --watchAll=false",
    "test:frontend:watch": "react-scripts test",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "npm run test:api && npm run test:frontend && npm run test:e2e",
    "test:coverage": "npm run test:api -- --coverage && npm run test:frontend -- --coverage",
    "playwright:install": "playwright install"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.8.0",
    "@types/supertest": "^2.0.16",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "gh-pages": "^6.0.0",
    "jest-environment-jsdom": "^30.1.2",
    "nodemon": "^3.0.2",
    "playwright": "^1.55.0",
    "postcss": "^8.4.31",
    "supertest": "^6.3.3",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.1",
    "vitest": "^1.0.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
